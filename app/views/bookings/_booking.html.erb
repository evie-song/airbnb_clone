<div id="<%= dom_id booking %>">
  <p>
    <strong>User:</strong>
    <%= booking.user_id %>
  </p>

  <p>
    <strong>Listing:</strong>
    <%= booking.listing_id %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= booking.start_date %>
